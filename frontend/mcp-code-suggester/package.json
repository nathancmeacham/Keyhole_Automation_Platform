{
    "name": "mcp-code-suggester",
    "displayName": "MCP Code Suggester",
    "publisher": "Keyhole_Solution_MCP",
    "version": "0.0.1",
    "engines": {
      "vscode": "^1.85.0"
    },
    "categories": [
      "Other"
    ],
    "activationEvents": [
      "onCommand:mcp.requestSuggestions"
    ],
    "main": "./out/extension.js",
    "contributes": {
      "commands": [
        {
          "command": "mcp.requestSuggestions",
          "title": "MCP: Request Code Suggestions"
        }
      ]
    },
    "scripts": {
      "compile": "tsc -p ./",
      "watch": "tsc -watch -p ./",
      "prepublish": "npm run compile"
    },
    "devDependencies": {
      "@types/vscode": "^1.85.0",
      "@types/node": "^20.x",
      "typescript": "^5.x",
      "vscode-test": "^1.x"
    },
    "dependencies": {
      "axios": "^1.6.5"
    }
  }